{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "pre-build": {},
    "build": {
      "dependsOn": ["pre-build"]
    },
    "package": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "lint": {
      "outputs": []
    }
  }
}